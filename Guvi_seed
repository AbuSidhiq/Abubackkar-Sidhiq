package empty;
import java.util.*;
public class Guvi_seed {

	    public static void main(String args[])
	    {
	    
	        Scanner sc=new Scanner(System.in);
	        long number=sc.nextLong();
	        long rem=0,sum=number;
	        while(number>0)
	        {
	             rem=number%10;
	             sum=sum*rem;
	            number=number/10;
	        }
	        System.out.print(sum);
	    }
}
